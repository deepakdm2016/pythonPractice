
marks=[float(x) for x in input("Enter Math, Physics, Chemistry marks seperated by comma").split()]
print(marks)

total=0.0
for m in marks:
        total=total+m

if (marks[0]<35 or marks[1]<35 or marks[2]<35):
    print("you have failed in the examination")

elif (total/3<=59):
    print("Your grade is C")
    
elif (total/3<=69):
    print("Your grade is B")
    
else:
    print("Your grade is A")



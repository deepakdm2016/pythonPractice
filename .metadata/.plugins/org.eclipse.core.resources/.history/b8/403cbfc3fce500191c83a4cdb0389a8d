import re

str="Take up One 123 idea at One time"
result=re.search(r'o\w\w', str)
#print(result.group())
print(result)

result=re.findall(r'o\w\w', str)
print(result)

result=re.match(r'T\w\w', str)
print(result)
print(result.group())

print(str.split(" "))

result=re.split(r'\d+', str)
print(result)

result=re.sub(r'One', r'Two', str)
print(result)
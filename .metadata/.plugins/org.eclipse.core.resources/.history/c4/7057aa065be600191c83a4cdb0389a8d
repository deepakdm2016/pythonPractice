s=input("Please enter the string")
words=s.split()
uniqueWords={}
print(type(uniqueWords))
uniqueWords=set()
print(type(uniqueWords))

for w in words:
    uniqueWords.add(w)
#uniqueWords.sorted()
print(uniqueWords)
print(sorted(uniqueWords, key=None, reverse=False))


output=""
for s in [sorted(uniqueWords, key=None, reverse=False)]:
    print(s)
    output+=s+" "
print(output)

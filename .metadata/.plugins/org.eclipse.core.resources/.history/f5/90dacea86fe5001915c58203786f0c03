from abc import abstractmethod,ABC
class TouchScreenLaptop(ABC):
    
    def __init__(self):
        print("Touch screen laptop")
        
    @abstractmethod
    def Scroll(self):
        pass
    
    @abstractmethod
    def Click(self):
        pass
    
class HP(TouchScreenLaptop):
    
    def __init__(self):
        print("HP laptop")
        
    
    def Scroll(self):
        print("Scroll HP laptop")
        
class Dell(TouchScreenLaptop):
    
    def __init__(self):
        print("Dell laptop")
        
    
    def Scroll(self):
        print("Scroll Dell laptop")